{
  "manifest_version": 2,
  "name": "SumTab",
  "description": "Keep your tabs alive. Don't mark what you don't need. The most useful tool for busy people. SumTab.",
  "version": "1.0",
   //"icons": {
     //"16": "icon16.png",
     //"48": "icon48.png",
     //"128": "icon128.png"
            //},
  "browser_action": {
    "default_title": "SumTab",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}